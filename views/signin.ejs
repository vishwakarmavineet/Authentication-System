<link rel="stylesheet" href="/css/signin.css">
<div id="signin-container">
<h1>Login</h1>

<form action="/users/create-session" method="POST">
    <input type="email" name="email" id="signin-email" required placeholder="Email">
    <input type="password" name="password" id="signin-password" required placeholder="Password">
    <button type="submit" id="signin-btn">Sign In</button>
</form>
<p>Don't have an account? <a href="/users/signup">Register Here</a></p>
<p>
    <a href="/users/auth/google">Google Sign In/ Sign Up</a>
</p>
</div>
